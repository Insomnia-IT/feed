        "workbox-core": "^6.5.3",
        "workbox-expiration": "^6.5.3",
        "workbox-google-analytics": "^6.5.3",
        "workbox-navigation-preload": "^6.5.3",
        "workbox-precaching": "^6.5.3",
        "workbox-range-requests": "^6.5.3",
