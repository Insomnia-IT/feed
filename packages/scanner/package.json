{
    "name": "@feed/scanner",
    "version": "0.0.1",
    "private": true,
    "homepage": "/cl421io",
    "installConfig": {
        "hoistingLimits": "workspaces"
    },
    "nohoist": [
        "@craco/craco",
        "@craco/craco/**",
        "react-scripts",
        "react-scripts/**"
    ],
    "dependencies": {
        "@craco/craco": "^7.0.0",
