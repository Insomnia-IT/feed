    "scripts": {
        "start": "BROWSER=none CLIENT_ENV=development craco start --verbose",
        "build": "CLIENT_ENV=production craco build --verbose",
        "bl": "browserslist --update-db",
        "lint:css": "stylelint ./src/**/*.css",
        "lint-fix:css": "stylelint --fix ./src/**/*.css",
        "lint:js": "eslint -c .eslintrc --ext .ts --ext .tsx src",
        "lint-fix:js": "eslint -c .eslintrc --fix --ext .ts --ext .tsx src",
        "lint": "npm-run-all -s lint:*",
        "lint-fix": "npm-run-all -s lint-fix:*",
        "tc": "tsc --noemit --emitDeclarationOnly false"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/plugin-transform-react-jsx": "^7.17.12",
        "@craco/craco": "^7.0.0",
        "@jackwilsdon/craco-use-babelrc": "^1.0.0",
        "@typescript-eslint/eslint-plugin": "^5.28.0",
        "@typescript-eslint/parser": "^5.28.0",
        "@welldone-software/why-did-you-render": "^7.0.1",
        "babel-plugin-module-resolver": "^4.1.0",
        "browserslist": "^4.20.4",
        "copy-webpack-plugin": "^11.0.0",
        "craco-raw-loader": "^1.0.2",
        "craco-swc": "^0.5.1",
        "postcss": "^8.4.14",
        "postcss-clamp": "^4.1.0",
        "postcss-hexrgba": "^2.1.0",
        "postcss-import": "^14.1.0",
        "postcss-loader": "^7.0.0",
        "postcss-media-minmax": "^5.0.0",
        "postcss-scss": "^4.0.4",
        "react-app-alias": "^2.2.0",
        "react-app-alias-ex": "^2.1.0",
        "stylelint-color-format": "^1.1.0"
    }
