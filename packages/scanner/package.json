        "exports-loader": "^4.0.0",
        "imports-loader": "^4.0.0",
        "onscan.js": "^1.5.2",
        "postcss-mixins": "^9.0.3",
        "qr-scanner": "^1.4.1",
        "raw-loader": "^4.0.2",
        "react-error-boundary": "^3.1.4",
        "react-scripts": "^5.0.1",
        "ulid": "^2.3.0",
        "workbox-background-sync": "^6.5.3",
        "workbox-broadcast-update": "^6.5.3",
